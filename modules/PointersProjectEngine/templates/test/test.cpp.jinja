                       {{ private_items }}_backup[i].{{ item_name }}_name) == 0)
        << "The {{ item_name }} names in the array should not be changed";
  }
}

void testLinkedList({{ node_type }}* node, int index[], int l) {
  int i;
  for (i = 0; i < l && node != NULL; i++, node = node->next) {
    // printf("TEST: %s vs %s\n", node->{{ item_name }}->{{ item_name }}_name,
    // {{ private_items }}[index[i]].{{ item_name }}_name);
    EXPECT_TRUE(node->{{ item_name }} == &{{ private_items }}[index[i]])
        << "The {{ cluster_name }} has incorrect nodes or order of nodes in the "
           "linkedList";
  }

  EXPECT_TRUE(node == NULL) << "One or more {{ container_name }} in the {{ cluster_name }} have a "
                               "longer linkedList than expected "
                            << l;
  EXPECT_EQ(l, i) << "One or more {{ container_name }} in the {{ cluster_name }} have a shorter "
                     "linkedList than expected "
                  << l;
}

int contains({{ node_type }}* head, const char* {{ item_name }}name) {
  for ({{ node_type }}* cur = head; cur; cur = cur->next) {
    if (!strncmp(cur->{{ item_name }}->{{ item_name }}_name, {{ item_name }}name, MAX_LEN)) {
      return 1;
    }
  }
  return 0;
}

